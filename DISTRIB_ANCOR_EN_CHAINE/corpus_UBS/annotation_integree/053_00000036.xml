          <anchor id="jmuzerelle_1378222993378" num="14">il</anchor>
          <anchor id="jmuzerelle_1378222997668" num="15">vous</anchor>
          est parvenu ou pas encore
        </Turn>
        <Turn speaker="spk1" startTime="18.443" endTime="19.76" id="5">
