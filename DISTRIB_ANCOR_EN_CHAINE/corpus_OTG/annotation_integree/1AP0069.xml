          <anchor id="jmuzerelle_1368798939499" num="15">
            un mois de
            <anchor id="jmuzerelle_1368798949353" num="16">délai</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk2" startTime="11.556" endTime="13.211" id="6">
          <Sync time="11.556" />
          d'accord ben
          <anchor id="jmuzerelle_1368798961155" num="17">je</anchor>
