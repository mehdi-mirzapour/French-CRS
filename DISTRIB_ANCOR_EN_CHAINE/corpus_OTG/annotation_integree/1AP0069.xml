          <anchor id="jmuzerelle_1368798857488" num="5">en</anchor>
          a plus
        </Turn>
        <Turn speaker="spk1" startTime="4.284" endTime="11.556" id="5">
          <Sync time="4.284" />
          hum non [i] et e
          <anchor id="jmuzerelle_1368798866588" num="6">
            la communauté de
            <anchor id="jmuzerelle_1368798873212" num="7">communes</anchor>
          </anchor>
