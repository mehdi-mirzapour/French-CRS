            <anchor id="gpascault_1352726670119" num="6">cinéma</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk1" startTime="3.471" endTime="6.331" id="4">
          <Sync time="3.471" />
          ouais # il y a pas
          <anchor id="gpascault_1352726697059" num="7">d'adresses</anchor>
          c'est sûr
        </Turn>
        <Turn speaker="spk2" startTime="6.331" endTime="8.346" id="5">
          <Sync time="6.331" />
          ah hé ben à part
          <anchor id="gpascault_1352726767425" num="8">les cinémas</anchor>
          <anchor id="gpascault_1352726796119" num="9">je</anchor>
          vois pas e
        </Turn>
        <Turn speaker="spk1 spk2" startTime="8.346" endTime="8.815" id="6">
          <Sync time="8.346" />
          <Who nb="1" />
          ah ouais
          <Who nb="2" />
          ou sinon
        </Turn>
        <Turn speaker="spk2" startTime="8.815" endTime="9.775" id="7">
          <Sync time="8.815" />
          <anchor id="gpascault_1352726917562" num="10">les Decaux</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="9.775" endTime="10.555" id="8">
          <Sync time="9.775" />
          où ça
        </Turn>
        <Turn speaker="spk2" startTime="10.555" endTime="15.39" id="9">
          <Sync time="10.555" />
          <anchor id="gpascault_1352726986305" num="11">la maison Decaux</anchor>
          <anchor id="gpascault_1352727030248" num="12">qui</anchor>
          est
          <anchor id="gpascault_1352727044646" num="13">
            rue
            <anchor id="gpascault_1352727087854" num="14">du Vercors</anchor>
          </anchor>
          quand
          <anchor id="gpascault_1352727144257" num="15">vous</anchor>
          allez e
          <anchor id="gpascault_1352727160324" num="16">
            direction
            <anchor id="gpascault_1352727214756" num="17">Fontaine</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk1" startTime="15.39" endTime="16.114" id="10">
          <Sync time="15.39" />
          ah d'accord
        </Turn>
        <Turn speaker="spk2" startTime="16.114" endTime="29.727" id="11">
          <Sync time="16.114" />
          hein
          <anchor id="gpascault_1352727246289" num="18">
            la rue
            <anchor id="gpascault_1352727333658" num="19">du Vercors</anchor>
          </anchor>
          <anchor id="jmuzerelle_1369228960719" num="20">c'</anchor>
          est
          <anchor id="gpascault_1352727357441" num="21">la rue</anchor>
          <anchor id="gpascault_1352727368699" num="22">qui</anchor>
          se trouve e derrière
          <anchor id="gpascault_1352727419874" num="23">le knack</anchor>
          +[mot inconnu] là bas # et alors e
          <anchor id="gpascault_1352727566952" num="24">au numéro trente-sept</anchor>
          <anchor id="gpascault_1352727628132" num="25">vous</anchor>
          avez
          <anchor id="gpascault_1352727633460" num="26">la maison Decaux</anchor>
          donc
          <anchor id="gpascault_1352727681177" num="27">qui</anchor>
          fait
          <anchor id="gpascault_1352727707066" num="28">
            tous les affichages
            <anchor id="gpascault_1352727764253" num="29">des panneaux</anchor>
          </anchor>
          #
          <anchor id="gpascault_1352727826619" num="30">eux</anchor>
          e peut être
          <anchor id="gpascault_1352727854044" num="31">vous</anchor>
          donneront e
          <anchor id="gpascault_1352727878061" num="32">des renseignements</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="29.727" endTime="33.599" id="12">
          <Sync time="29.727" />
          près en allant vers [pi]
        </Turn>
        <Turn speaker="spk2" startTime="33.599" endTime="35.997" id="13">
          <Sync time="33.599" />
          voilà voyez regardez
        </Turn>
        <Turn speaker="spk1" startTime="35.997" endTime="36.668" id="14">
          <Sync time="35.997" />
          ah oui d'accord
        </Turn>
        <Turn speaker="spk2" startTime="36.668" endTime="37.672" id="15">
          <Sync time="36.668" />
          <anchor id="jmuzerelle_1369229033628" num="33">c'</anchor>
          est par là
          <anchor id="gpascault_1352727908843" num="34">je</anchor>
          crois que
          <anchor id="jmuzerelle_1369229040604" num="35">c'</anchor>
          est
          <anchor id="gpascault_1352727939742" num="36">au numéro</anchor>
        </Turn>
        <Turn speaker="spk1 spk2" startTime="37.672" endTime="38.147" id="16">
          <Sync time="37.672" />
          <Who nb="1" />
          ah oui
          <Who nb="2" />
          <anchor id="gpascault_1352727986541" num="37">trente-sept</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="38.147" endTime="39.886" id="17">
          <Sync time="38.147" />
          e ouais d'accord
          <anchor id="jmuzerelle_1369229092616" num="38">c'</anchor>
          est là où passe
          <anchor id="gpascault_1352728048088" num="39">le cinq</anchor>
          d'accord
        </Turn>
        <Turn speaker="spk2" startTime="39.886" endTime="40.486" id="18">
          <Sync time="39.886" />
          voilà
        </Turn>
        <Turn speaker="spk1" startTime="40.486" endTime="41.654" id="19">
          <Sync time="40.486" />
          <anchor id="gpascault_1352728070335" num="40">je</anchor>
