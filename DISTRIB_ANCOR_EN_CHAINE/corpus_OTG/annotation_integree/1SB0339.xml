          <Sync time="0" />
          bonjour
        </Turn>
        <Turn speaker="spk2" startTime="0.744" endTime="3.387" id="2">
          <Sync time="0.744" />
          bonjour
          <anchor id="gpascault_1353064361597" num="1">je</anchor>
          viens chercher
          <anchor id="gpascault_1353064364820" num="2">des plans</anchor>
          pour
          <anchor id="gpascault_1353064403018" num="3">un hôtel</anchor>
        </Turn>
        <Turn speaker="spk1 spk2" startTime="3.387" endTime="4.157" id="3">
          <Sync time="3.387" />
          <Who nb="1" />
          <anchor id="gpascault_1353064419887" num="4">vous</anchor>
          avez commandé
          <Who nb="2" />
          <anchor id="gpascault_1353064423896" num="5">je</anchor>
          me rappelle plus
        </Turn>
        <Turn speaker="spk2" startTime="4.157" endTime="5.879" id="4">
          <Sync time="4.157" />
          oui
          <anchor id="gpascault_1353064461058" num="6">"château de la Commanderie"</anchor>
        </Turn>
        <Turn speaker="spk1 spk2" startTime="5.879" endTime="6.731" id="5">
          <Sync time="5.879" />
          <Who nb="1" />
          possible
          <Who nb="2" />
          et ben
        </Turn>
        <Turn speaker="spk2" startTime="6.731" endTime="7.389" id="6">
          <Sync time="6.731" />
          voyez
        </Turn>
        <Turn speaker="spk1" startTime="7.389" endTime="9.06" id="7">
          <Sync time="7.389" />
          ah
          <anchor id="gpascault_1353064501627" num="7">il</anchor>
          faut que
          <anchor id="gpascault_1353064514121" num="8">vous</anchor>
          montiez
          <anchor id="gpascault_1353064521085" num="9">au troisième</anchor>
          par contre
        </Turn>
        <Turn speaker="spk2 spk1" startTime="9.06" endTime="10.306" id="8">
          <Sync time="9.06" />
          <Who nb="1" />
          <anchor id="gpascault_1353157441826" num="10">troisième étage</anchor>
          <anchor id="jmuzerelle_1370960500305" num="11">c'</anchor>
          est
          <Who nb="2" />
          d'accord
          <anchor id="gpascault_1353157530932" num="12">vous</anchor>
          avez
          <anchor id="gpascault_1353157538948" num="13">un ascenseur</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="10.306" endTime="10.812" id="9">
          <Sync time="10.306" />
          juste après
          <anchor id="gpascault_1353157567960" num="14">le</anchor>
        </Turn>
        <Turn speaker="spk2 spk1" startTime="10.812" endTime="11.435" id="10">
          <Sync time="10.812" />
          <Who nb="1" />
          d'accord
          <Who nb="2" />
          <anchor id="gpascault_1353157562465" num="15">Cargo</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="11.435" endTime="12.126" id="11">
          <Sync time="11.435" />
          merci beaucoup
        </Turn>
        <Turn speaker="spk1" startTime="12.126" endTime="12.482" id="12">
          <Sync time="12.126" />
          au revoir
        </Turn>
      </Section>
