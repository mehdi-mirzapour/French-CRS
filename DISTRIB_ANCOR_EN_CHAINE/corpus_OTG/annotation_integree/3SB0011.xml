    <Episode>
      <Section type="report" startTime="0" endTime="39.101" id="1">
        <Turn startTime="0" endTime="3.011" speaker="spk1" id="1">
          <Sync time="0" />
          est ce qu'
          <anchor id="adrouin_1338584602747" num="1">on</anchor>
          pourrait avoir
          <anchor id="adrouin_1338584615396" num="2">
            cinq plans de
            <anchor id="adrouin_1338584630627" num="3">Grenoble</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk2" startTime="3.011" endTime="5.14" id="2">
          <Sync time="3.011" />
          hein cinq
          <anchor id="adrouin_1338584654947" num="4">je</anchor>
          sais pas hein
        </Turn>
        <Turn speaker="spk1" startTime="5.14" endTime="7.315" id="3">
          <Sync time="5.14" />
          plus
          <anchor id="adrouin_1338584668539" num="5">les pistes cyclables</anchor>
          aussi
        </Turn>
        <Turn speaker="spk2" startTime="7.315" endTime="8.259" id="4">
          <Sync time="7.315" />
          <anchor id="adrouin_1338584807569" num="6">ils</anchor>
          <anchor id="adrouin_1338584733816" num="7">en</anchor>
          ont plus
        </Turn>
        <Turn speaker="spk1" startTime="8.259" endTime="8.787" id="5">
          <Sync time="8.259" />
          plus
        </Turn>
        <Turn speaker="spk2" startTime="8.787" endTime="13.65" id="6">
          <Sync time="8.787" />
          donc #
          <anchor id="adrouin_1338584815730" num="8">ils</anchor>
          reéditent
          <anchor id="adrouin_1338584757849" num="9">leur stock</anchor>
          donc d'ici
          <anchor id="adrouin_1338584773853" num="10">un mois</anchor>
          <anchor id="jmuzerelle_1338887632119" num="11">ils</anchor>
          <anchor id="adrouin_1338584794970" num="12">nous</anchor>
          ont dit un peu plus
          <anchor id="adrouin_1338584836144" num="13">je</anchor>
          sais plus
        </Turn>
        <Turn speaker="spk1" startTime="13.65" endTime="19.713" id="7">
          <Sync time="13.65" />
          et puis e
          <anchor id="adrouin_1338584857943" num="14">des "sciences en fête</anchor>
          "
          <anchor id="jmuzerelle_1371112461258" num="15">qu'</anchor>
          arrivent e en
          <anchor id="adrouin_1338584900468" num="16">septembre</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="19.713" endTime="20.426" id="8">
          <Sync time="19.713" />
          cinq aussi
        </Turn>
        <Turn speaker="spk1" startTime="20.426" endTime="24.925" id="9">
          <Sync time="20.426" />
          ouais
        </Turn>
        <Turn speaker="spk2" startTime="24.925" endTime="25.547" id="10">
          <Sync time="24.925" />
          voilà
        </Turn>
        <Turn speaker="spk1" startTime="25.547" endTime="27.845" id="11">
          <Sync time="25.547" />
          merci
        </Turn>
        <Turn speaker="spk2" startTime="27.845" endTime="31.82" id="12">
          <Sync time="27.845" />
          ça c'est
          <anchor id="adrouin_1338584944987" num="17">
            les marque-pages # "
            <anchor id="adrouin_1338584968673" num="18">sciences en fête</anchor>
          </anchor>
          "
          <anchor id="adrouin_1338585012138" num="19">il</anchor>
          <anchor id="adrouin_1338585024052" num="20">nous</anchor>
          <anchor id="adrouin_1338585035097" num="21">en</anchor>
          reste
          <anchor id="jmuzerelle_1338887790391" num="22">quelques uns</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="31.82" endTime="32.606" id="13">
          <Sync time="31.82" />
          merci
        </Turn>
        <Turn speaker="spk2" startTime="32.606" endTime="37.083" id="14">
          <Sync time="32.606" />
          ça c'est les #
          <anchor id="adrouin_1338585056122" num="23">je</anchor>
          peux pas
          <anchor id="adrouin_1338585062302" num="24">vous</anchor>
          <anchor id="adrouin_1338585070658" num="25">en</anchor>
          donner trop
          <anchor id="adrouin_1338585148973" num="26">je</anchor>
          <anchor id="adrouin_1338585156972" num="27">vous</anchor>
          <anchor id="adrouin_1338585161824" num="28">en</anchor>
          donne qu'
          <anchor id="adrouin_1338585172146" num="29">une paire</anchor>
          <anchor id="adrouin_1338585195723" num="30">c'</anchor>
          est pour observer la
          <anchor id="adrouin_1338585208249" num="31">
            l'éclipse de
            <anchor id="adrouin_1338585220095" num="32">lune</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk1" startTime="37.083" endTime="38.746" id="15">
          <Sync time="37.083" />
          <anchor id="adrouin_1338585235497" num="33">je</anchor>
          ne
          <anchor id="adrouin_1338585243483" num="34">la</anchor>
          donnerai pas merci
        </Turn>
        <Turn speaker="spk2" startTime="38.746" endTime="39.101" id="16">
          <Sync time="38.746" />
          voilà
        </Turn>
      </Section>
    </Episode>
