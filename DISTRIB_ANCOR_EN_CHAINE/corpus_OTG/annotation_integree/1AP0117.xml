    <Episode>
      <Section type="report" startTime="0" endTime="13.260" topic="to1" id="1">
        <Turn startTime="0" endTime="0.484" speaker="spk1 spk2" id="1">
          <Sync time="0" />
          <Who nb="1" />
          bonjour
          <Who nb="2" />
          est ce
        </Turn>
        <Turn speaker="spk2" startTime="0.484" endTime="5.827" id="2">
          <Sync time="0.484" />
          que
          <anchor id="sduchon_1340375412211" num="1">vous</anchor>
          savez comment
          <anchor id="sduchon_1340375433989" num="2">il</anchor>
          faut faire pour aller
          <anchor id="sduchon_1340375642140" num="3">
            au col
            <anchor id="sduchon_1340375653372" num="4">du Barrioz</anchor>
          </anchor>
          est ce qu'
          <anchor id="sduchon_1340375577025" num="5">il</anchor>
          faut passer par
          <anchor id="sduchon_1340375784755" num="6">Saint Pierre d'Allevard</anchor>
        </Turn>
        <Turn speaker="spk1 spk2" startTime="5.827" endTime="6.694" id="3">
          <Sync time="5.827" />
          <Who nb="1" />
          non alors [rire]
          <Who nb="2" />
          ou par
          <anchor id="jmuzerelle_1342440323045" num="7">Theys</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="6.694" endTime="11.419" id="4">
          <Sync time="6.694" />
          ce que
          <anchor id="sduchon_1340375597789" num="8">je</anchor>
          <anchor id="sduchon_1340375605012" num="9">vous</anchor>
          conseille de faire c'est voir avec "
          <anchor id="sduchon_1340375903346" num="10">info montagne</anchor>
          " regardez
          <anchor id="sduchon_1340375614450" num="11">ils</anchor>
          sont juste au dessus SIM
          <anchor id="sduchon_1340376009021" num="12">au premier étage</anchor>
        </Turn>
        <Turn speaker="spk2 spk1" startTime="11.419" endTime="12.565" id="5">
          <Sync time="11.419" />
          <Who nb="1" />
          merci merci
          <Who nb="2" />
          il y a
          <anchor id="sduchon_1340376186283" num="13">un ascenseur</anchor>
          hein
        </Turn>
        <Turn speaker="spk1" startTime="12.565" endTime="13.260" id="6">
          <Sync time="12.565" />
          <anchor id="sduchon_1340375627897" num="14">je</anchor>
          <anchor id="sduchon_1340375631828" num="15">vous</anchor>
          en prie
          <anchor id="sduchon_1340376197094" num="16">monsieur</anchor>
        </Turn>
      </Section>
    </Episode>
