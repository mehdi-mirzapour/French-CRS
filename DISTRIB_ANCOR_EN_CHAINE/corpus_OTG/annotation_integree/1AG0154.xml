    <Episode program="" air_date="">
      <Section type="report" startTime="0" endTime="16.280" topic="to1" id="1">
        <Turn startTime="0" endTime="1.265" speaker="spk2" id="1">
          <Sync time="0" />
          <anchor id="sduchon_1329150511013" num="1">je</anchor>
          peux m'occuper de
          <anchor id="sduchon_1329151045154" num="2">quelqu'un</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="1.265" endTime="5.055" id="2">
          <Sync time="1.265" />
          <anchor id="sduchon_1329151156465" num="3">moi</anchor>
          <anchor id="sduchon_1329150520483" num="4">je</anchor>
          veux bien c'est
          <anchor id="sduchon_1329150525343" num="5">je</anchor>
          cherche
          <anchor id="sduchon_1329151175385" num="6">
            une carte
            <anchor id="sduchon_1329151188155" num="7">
              des massifs montagneux de
              <anchor id="sduchon_1329151204575" num="8">Grenoble</anchor>
            </anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk2" startTime="5.055" endTime="12.285" id="3">
          <Sync time="5.055" />
          e
          <anchor id="sduchon_1329151269405" num="9">il</anchor>
          faudra monter à
          <anchor id="sduchon_1329402565484" num="10">"info montagne"</anchor>
          parce que
          <anchor id="jmuzerelle_1329319515907" num="11">moi</anchor>
          <anchor id="sduchon_1329150533893" num="12">j'</anchor>
          ai
          <anchor id="sduchon_1329402541124" num="13">ça</anchor>
          et
          <anchor id="sduchon_1329150539663" num="14">j'</anchor>
          ai peur que
          <anchor id="jmuzerelle_1370593798772" num="15">ce</anchor>
          soit pas assez compliqué à moins que
          <anchor id="sduchon_1329402693901" num="16">ça</anchor>
          <anchor id="sduchon_1329150555413" num="17">vous</anchor>
          suffise
        </Turn>
        <Turn speaker="spk1" startTime="12.285" endTime="14.079" id="4">
          <Sync time="12.285" />
          <anchor id="sduchon_1329150560243" num="18">je</anchor>
          vais déjà prendre
          <anchor id="sduchon_1329402610230" num="19">ça</anchor>
          sinon
          <anchor id="sduchon_1329152826256" num="20">il</anchor>
          faut que
          <anchor id="sduchon_1329150563053" num="21">je</anchor>
          monte
        </Turn>
        <Turn speaker="spk2" startTime="14.079" endTime="16.280" id="5">
          <Sync time="14.079" />
          oui
          <anchor id="sduchon_1329402649373" num="22">"info montagne</anchor>
          " # voilà
          <anchor id="sduchon_1329152735401" num="23">madame</anchor>
        </Turn>
      </Section>
    </Episode>
