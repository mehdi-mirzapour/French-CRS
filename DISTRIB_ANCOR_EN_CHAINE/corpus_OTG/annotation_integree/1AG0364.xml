    <Episode>
      <Section type="report" startTime="0" endTime="15.418" topic="to1" id="1">
        <Turn startTime="0" endTime="0.71" speaker="spk1 spk2" id="1">
          <Sync time="0" />
          <Who nb="1" />
          bonjour
          <Who nb="2" />
          bonjour
        </Turn>
        <Turn speaker="spk1" startTime="0.71" endTime="13.312" id="2">
          <Sync time="0.71" />
          dites est ce que
          <anchor id="sduchon_1329165825702" num="1">vous</anchor>
          avez un
          <anchor id="sduchon_1329166583476" num="2">des informations</anchor>
          chez
          <anchor id="sduchon_1329166597658" num="3">des expositions</anchor>
          <anchor id="sduchon_1329165831669" num="4">qui</anchor>
          sont à
          <anchor id="sduchon_1329166623697" num="5">la bibliothèque</anchor>
          # actuellement il y a
          <anchor id="sduchon_1329167010963" num="6">quelque chose</anchor>
          sur
          <anchor id="sduchon_1329166696996" num="7">la Salette</anchor>
          et
          <anchor id="sduchon_1329166676518" num="8">les horaires</anchor>
          <anchor id="sduchon_1329166389124" num="9">j'</anchor>
          ai vu
          <anchor id="sduchon_1329421507398" num="10">ça</anchor>
          sur
          <anchor id="sduchon_1329166756777" num="11">un journal</anchor>
          <anchor id="sduchon_1329421577890" num="12">c'</anchor>
          est pas forcément
          <anchor id="sduchon_1329421344453" num="13">tous les jours</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="13.312" endTime="14.265" id="3">
          <Sync time="13.312" />
          voilà
          <anchor id="sduchon_1329166532646" num="14">vous</anchor>
          <anchor id="sduchon_1329421898144" num="15">l'</anchor>
          avez là dedans
        </Turn>
        <Turn speaker="spk1" startTime="14.265" endTime="15.418" id="4">
          <Sync time="14.265" />
          là dessus ouais merci
        </Turn>
      </Section>
    </Episode>
