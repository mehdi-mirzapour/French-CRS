        </Turn>
        <Turn speaker="spk1 spk2" startTime="9.22" endTime="10.114" id="7">
          <Sync time="9.22" />
          <Who nb="1" />
          ici
          <Who nb="2" />
          <anchor id="mgodefroy_1331836510662" num="11">
            lundi
            <anchor id="mgodefroy_1331836498868" num="12">matin</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk1" startTime="10.114" endTime="15.522" id="8">
          <Sync time="10.114" />
          e
          <anchor id="mgodefroy_1331836530989" num="13">matin</anchor>
          ou
          <anchor id="mgodefroy_1331836542515" num="14">après midi</anchor>
          ça dépend un peu de
          <anchor id="mgodefroy_1331836571091" num="15">leur tournée</anchor>
          quoi c'est jamais e le
          <anchor id="mgodefroy_1331836602424" num="16">la même heure</anchor>
          quoi
        </Turn>
        <Turn speaker="spk2" startTime="15.522" endTime="16.082" id="9">
          <Sync time="15.522" />
          d'accord
        </Turn>
        <Turn speaker="spk1 spk2" startTime="16.082" endTime="16.366" id="10">
          <Sync time="16.082" />
          <Who nb="1" />
          <anchor id="mgodefroy_1331836647877" num="17">il</anchor>
          <Who nb="2" />
          merci
        </Turn>
        <Turn speaker="spk1" startTime="16.366" endTime="19.89" id="11">
          <Sync time="16.366" />
          faut vraiment passer
          <anchor id="mgodefroy_1331836675241" num="18">le lundi</anchor>
          pour avoir
          <anchor id="mgodefroy_1331836710943" num="19">"le 38</anchor>
          " hein parce que
          <anchor id="mgodefroy_1331836729580" num="20">le lendemain</anchor>
          <anchor id="mgodefroy_1331836743802" num="21">on</anchor>
          <anchor id="mgodefroy_1331836762656" num="22">en</anchor>
