        <Turn speaker="spk2" startTime="2.964" endTime="6.362" id="4">
          <Sync time="2.964" />
          <anchor id="mgodefroy_1331836355918" num="3">un appartement</anchor>
          <anchor id="mgodefroy_1331836369047" num="4">la semaine prochaine</anchor>
          <anchor id="mgodefroy_1331836387200" num="5">j'</anchor>
          aimerais savoir
          <anchor id="mgodefroy_1331836403396" num="6">quel jour</anchor>
          sort "
          <anchor id="mgodefroy_1331836420573" num="7">le 38"</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="6.362" endTime="8.639" id="5">
          <Sync time="6.362" />
          <anchor id="mgodefroy_1331836438664" num="8">"le 38"</anchor>
          <anchor id="mgodefroy_1331836451573" num="9">nous</anchor>
          est livré
          <anchor id="mgodefroy_1331836462601" num="10">le lundi</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="8.639" endTime="9.22" id="6">
          <Sync time="8.639" />
