    <Episode>
      <Section type="report" startTime="0" endTime="13.611" topic="to1" id="1">
        <Turn startTime="0" endTime="0.326" speaker="spk1" id="1">
          <Sync time="0" />
          bonjour
        </Turn>
        <Turn speaker="spk2" startTime="0.326" endTime="6.404" id="2">
          <Sync time="0.326" />
          bonjour est ce que
          <anchor id="adrouin_1343424573335" num="1">vous</anchor>
          auriez
          <anchor id="adrouin_1343424581239" num="2">
            un plan de
            <anchor id="adrouin_1343424594718" num="3">Grenoble</anchor>
          </anchor>
          mais surtout
          <anchor id="adrouin_1343424611286" num="4">la banlieue</anchor>
          avec
          <anchor id="adrouin_1343424629483" num="5">
            les listes les
            <anchor id="adrouin_1343424645247" num="6">les villes</anchor>
            <anchor id="adrouin_1343424663675" num="7">les</anchor>
            hum # [bb]
            <anchor id="adrouin_1343424686993" num="8">rues</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk1" startTime="6.404" endTime="11.337" id="3">
          <Sync time="6.404" />
          e oui mais e pour
          <anchor id="adrouin_1343424713518" num="9">la banlieue</anchor>
          c'est pas
          <anchor id="adrouin_1343424752009" num="10">le plan détaillé</anchor>
          hein c'est juste
          <anchor id="adrouin_1343424783379" num="11">une localisation</anchor>
          par rapport à
          <anchor id="adrouin_1343424795751" num="12">Grenoble</anchor>
          hein
        </Turn>
        <Turn speaker="spk2" startTime="11.337" endTime="13.281" id="4">
          <Sync time="11.337" />
          d'accord # merci
        </Turn>
        <Turn speaker="spk1" startTime="13.281" endTime="13.611" id="5">
          <Sync time="13.281" />
          au revoir
        </Turn>
      </Section>
    </Episode>
