        </Turn>
        <Turn speaker="spk1" startTime="30.706" endTime="35.248" id="12">
          <Sync time="30.706" />
          sous
          <anchor id="gpascault_1352714782601" num="24">quel régime</anchor>
          <anchor id="gpascault_1352714817392" num="25">ils</anchor>
          sont
          <anchor id="gpascault_1352714892769" num="26">le CROUS</anchor>
          peut déjà
          <anchor id="gpascault_1352714908101" num="27">vous</anchor>
          aider après
          <anchor id="gpascault_1352714910990" num="28">vous</anchor>
          avez pris
          <anchor id="gpascault_1352714937847" num="29">"le 38"</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="35.248" endTime="35.552" id="13">
          <Sync time="35.248" />
          oui
        </Turn>
        <Turn speaker="spk1" startTime="35.552" endTime="38.366" id="14">
          <Sync time="35.552" />
          donc
          <anchor id="gpascault_1352715053075" num="30">ça</anchor>
          <anchor id="jmuzerelle_1369228301361" num="31">c'</anchor>
          est
          <anchor id="gpascault_1352715188796" num="32">
