          <Who nb="2" />
          bonjour
          <anchor id="gpascault_1352471799658" num="2">monsieur</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="0.766" endTime="7.083" id="2">
          <Sync time="0.766" />
          il y a
          <anchor id="gpascault_1352471812034" num="3">des associations</anchor>
