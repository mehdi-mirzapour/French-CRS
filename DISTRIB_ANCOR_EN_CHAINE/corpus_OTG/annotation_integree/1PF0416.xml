          <anchor id="gpascault_1352471950961" num="5">
            logements pour
            <anchor id="gpascault_1352472015781" num="6">les étudiants</anchor>
          </anchor>
          ou
        </Turn>
        <Turn speaker="spk1" startTime="7.083" endTime="17.93" id="3">
          <Sync time="7.083" />
          ben [pf]
          <anchor id="gpascault_1352472083575" num="7">je</anchor>
          vais
          <anchor id="gpascault_1352472094331" num="8">vous</anchor>
          donner
          <anchor id="gpascault_1352472100845" num="9">un petit bouquin</anchor>
          e que
          <anchor id="gpascault_1352472123577" num="10">que</anchor>
          <anchor id="gpascault_1352472155327" num="11">nous</anchor>
          distribuons qui
          <anchor id="gpascault_1352472178209" num="12">qui</anchor>
          sont en fait e #
          <anchor id="gpascault_1352714124609" num="13">ces livres</anchor>
          sont adressés à
          <anchor id="gpascault_1352714152234" num="14">la vie estudiantine</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="17.93" endTime="18.445" id="4">
          <Sync time="17.93" />
          oui
        </Turn>
        <Turn speaker="spk1" startTime="18.445" endTime="23.44" id="5">
          <Sync time="18.445" />
          et bon
          <anchor id="gpascault_1352714328474" num="15">ils</anchor>
          permettent de
          <anchor id="gpascault_1352714353460" num="16">vous</anchor>
          donner
          <anchor id="gpascault_1352714365452" num="17">
            un certain nombre de e d'
            <anchor id="gpascault_1352714436369" num="18">
              adresses d'
              <anchor id="gpascault_1352714494659" num="19">associations</anchor>
            </anchor>
          </anchor>
          e
        </Turn>
        <Turn speaker="spk2" startTime="23.44" endTime="23.899" id="6">
          <Sync time="23.44" />
          d'accord
        </Turn>
        <Turn speaker="spk1" startTime="23.899" endTime="26.415" id="7">
          <Sync time="23.899" />
          <anchor id="gpascault_1352714604575" num="20">il</anchor>
          faut surtout passer par
          <anchor id="gpascault_1352714638180" num="21">le CROUS</anchor>
          # déjà
        </Turn>
        <Turn speaker="spk2" startTime="26.415" endTime="26.98" id="8">
          <Sync time="26.415" />
          ouais
        </Turn>
        <Turn speaker="spk1 spk2" startTime="26.98" endTime="27.354" id="9">
          <Sync time="26.98" />
          <Who nb="1" />
          pour voir
          <Who nb="2" />
          ouais
        </Turn>
        <Turn speaker="spk1" startTime="27.354" endTime="30.295" id="10">
          <Sync time="27.354" />
          e si votre
          <anchor id="gpascault_1352714679567" num="22">vos enfants</anchor>
          ont
          <anchor id="gpascault_1352714731510" num="23">une bourse</anchor>
