          <anchor id="sduchon_1330017902358" num="16">vous</anchor>
          <anchor id="sduchon_1330017906801" num="17">le</anchor>
          trouverez dans
          <anchor id="sduchon_1330017451411" num="18">les mairies</anchor>
          essayez de faire
        </Turn>
        <Turn speaker="spk2 spk1" startTime="14.501" endTime="15.091" id="7">
