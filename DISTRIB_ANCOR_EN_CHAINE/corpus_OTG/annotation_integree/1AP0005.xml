          <anchor id="sduchon_1330017672665" num="25">un moment</anchor>
          <anchor id="jmuzerelle_1368793520082" num="26">qu'</anchor>
          <anchor id="sduchon_1330018624888" num="27">on</anchor>
          <anchor id="sduchon_1330018729486" num="28">en</anchor>
