        </Turn>
        <Turn speaker="spk2" startTime="6.013" endTime="9.149" id="4">
          <Sync time="6.013" />
          alors e si
          <anchor id="adrouin_1331038119707" num="7">vous</anchor>
          pouvez me renseigner
        </Turn>
        <Turn speaker="spk1 spk2" startTime="9.149" endTime="10.211" id="5">
          <Sync time="9.149" />
          <Who nb="1" />
          <anchor id="adrouin_1331038139519" num="8">je</anchor>
          vais
          <anchor id="adrouin_1331038146118" num="9">vous</anchor>
          laisser
          <anchor id="adrouin_1331038152264" num="10">un petit plan</anchor>
          <Who nb="2" />
          et puis
        </Turn>
        <Turn speaker="spk2" startTime="10.211" endTime="16.171" id="6">
          <Sync time="10.211" />
          <anchor id="adrouin_1331038174354" num="11">un plan</anchor>
          # et et pour
          <anchor id="adrouin_1331038188284" num="12">les chemins</anchor>
          parce que [pf] #
          <anchor id="adrouin_1331038209890" num="13">on</anchor>
          dit qu'il y a
          <anchor id="adrouin_1331038253726" num="14">
            des chemins de
            <anchor id="adrouin_1331039178011" num="15">randonnée</anchor>
          </anchor>
          là haut
        </Turn>
        <Turn speaker="spk1" startTime="16.171" endTime="21.528" id="7">
          <Sync time="16.171" />
          ce qu'
          <anchor id="adrouin_1331039219366" num="16">il</anchor>
          faudrait faire c'est que
          <anchor id="adrouin_1331039257009" num="17">vous</anchor>
          alliez voir "
          <anchor id="adrouin_1331039268163" num="18">info montagne</anchor>
          " à
          <anchor id="adrouin_1331039314698" num="19">
            la maison de
            <anchor id="adrouin_1331039334588" num="20">la randonnée</anchor>
          </anchor>
          <anchor id="adrouin_1331039348893" num="21">ils</anchor>
          sont au premier # donc
          <anchor id="adrouin_1331039459872" num="22">eux</anchor>
          <anchor id="adrouin_1331039501836" num="23">ils</anchor>
          vont
          <anchor id="adrouin_1331039550554" num="24">vous</anchor>
          e
        </Turn>
        <Turn speaker="spk2" startTime="21.528" endTime="22.778" id="8">
          <Sync time="21.528" />
          ça m'entraînera pour monter déjà
        </Turn>
        <Turn speaker="spk1" startTime="22.778" endTime="30.342" id="9">
          <Sync time="22.778" />
          voilà déjà #
          <anchor id="adrouin_1331039612159" num="25">ils</anchor>
          vont
          <anchor id="adrouin_1331039621971" num="26">vous</anchor>
          orienter
          <anchor id="adrouin_1331039627416" num="27">vous</anchor>
          donner ce qui faut parce que
          <anchor id="adrouin_1331039638726" num="28">moi</anchor>
          c'est surtout
          <anchor id="adrouin_1331039667164" num="29">des documents touristiques</anchor>
          pour
          <anchor id="adrouin_1331039772355" num="30">les</anchor>
          +[pronounce=laes]
          <anchor id="adrouin_1331039817907" num="31">téléphériques</anchor>
          <anchor id="adrouin_1331039833757" num="32">les horaires</anchor>
          et caetera et caetera
        </Turn>
        <Turn speaker="spk2" startTime="30.342" endTime="31.029" id="10">
          <Sync time="30.342" />
          d'accord déjà oui
        </Turn>
        <Turn speaker="spk1" startTime="31.029" endTime="35.027" id="11">
          <Sync time="31.029" />
          <anchor id="adrouin_1331039718878" num="33">on</anchor>
          <anchor id="adrouin_1331039937996" num="34">en</anchor>
          parle
          <anchor id="adrouin_1331039737583" num="35">on</anchor>
