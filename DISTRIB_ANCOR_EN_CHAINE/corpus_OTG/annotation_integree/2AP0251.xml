          <anchor id="adrouin_1331037952147" num="4">je</anchor>
          veux aller me balader
          <anchor id="adrouin_1331037977856" num="5">
            du côté de
            <anchor id="adrouin_1331038099942" num="6">la Bastille</anchor>
          </anchor>
        </Turn>
        <Turn speaker="spk1" startTime="5.723" endTime="6.013" id="3">
          <Sync time="5.723" />
