              <anchor id="adrouin_1338896799259" num="15">Lyon</anchor>
            </anchor>
          </anchor>
          aussi
        </Turn>
        <Turn speaker="spk2" startTime="12.865" endTime="15.361" id="4">
          <Sync time="12.865" />
          <anchor id="adrouin_1338896820768" num="16">je</anchor>
