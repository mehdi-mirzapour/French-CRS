        </Turn>
        <Turn speaker="spk1" startTime="6.232" endTime="12.865" id="3">
          <Sync time="6.232" />
          pour avoir
          <anchor id="adrouin_1338896707185" num="11">le programme</anchor>
          et aussi e
          <anchor id="adrouin_1338896746072" num="12">
