          <anchor id="adrouin_1338896883067" num="21">je</anchor>
          <anchor id="adrouin_1338896889591" num="22">vous</anchor>
        </Turn>
        <Turn speaker="spk1 spk2" startTime="17.297" endTime="17.594" id="7">
