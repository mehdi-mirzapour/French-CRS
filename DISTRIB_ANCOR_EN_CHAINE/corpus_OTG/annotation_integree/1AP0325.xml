    <Episode>
      <Section type="report" startTime="0" endTime="23.364" topic="to1" id="1">
        <Turn startTime="0" endTime="1.026" speaker="spk1" id="1">
          <Sync time="0" />
          bonjour
          <anchor id="sduchon_1342951301444" num="1">monsieur</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="1.026" endTime="9.112" id="2">
          <Sync time="1.026" />
          bonjour #
          <anchor id="sduchon_1342951672790" num="2">je</anchor>
          veux me renseigner e # pour avoir
          <anchor id="sduchon_1342951309724" num="3">un bouquin</anchor>
          <anchor id="sduchon_1342951680806" num="4">on</anchor>
          <anchor id="sduchon_1342951729867" num="5">m'</anchor>
          a dit qu'
          <anchor id="sduchon_1342951691981" num="6">il</anchor>
          existait
          <anchor id="sduchon_1342951360328" num="7">un bouquin</anchor>
          de e pour les [bb]
          <anchor id="sduchon_1342951385310" num="8">les chalets</anchor>
          <anchor id="sduchon_1342951391946" num="9">les trucs</anchor>
          comme ça
        </Turn>
        <Turn speaker="spk1 spk2" startTime="9.112" endTime="9.672" id="3">
          <Sync time="9.112" />
          <Who nb="1" />
          oui
          <anchor id="jmuzerelle_1369154814226" num="10">c'</anchor>
          est
          <Who nb="2" />
          dans
          <anchor id="sduchon_1342951437372" num="11">la région</anchor>
        </Turn>
        <Turn speaker="spk1 spk2" startTime="9.672" endTime="11.286" id="4">
          <Sync time="9.672" />
          <Who nb="1" />
          <anchor id="sduchon_1342951449035" num="12">Cléconfort</anchor>
          <Who nb="2" />
          <anchor id="sduchon_1342951469338" num="13">les réservations</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="11.286" endTime="12.882" id="5">
          <Sync time="11.286" />
          <anchor id="jmuzerelle_1369154832650" num="14">c'</anchor>
          est
          <anchor id="sduchon_1342951478833" num="15">Cléconfort</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="12.882" endTime="13.640" id="6">
          <Sync time="12.882" />
          ah ben voilà
        </Turn>
        <Turn speaker="spk1 spk2" startTime="13.640" endTime="14.079" id="7">
          <Sync time="13.640" />
          <Who nb="1" />
          ben voilà
          <Who nb="2" />
          <anchor id="sduchon_1342951742594" num="16">je</anchor>
          crois que
          <anchor id="jmuzerelle_1369154869013" num="17">c'</anchor>
          est
          <anchor id="sduchon_1342951746402" num="18">ça</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="14.079" endTime="15.195" id="8">
          <Sync time="14.079" />
          il y a
          <anchor id="sduchon_1342951557973" num="19">
            les numéros de
            <anchor id="sduchon_1342951567108" num="20">téléphone</anchor>
          </anchor>
          c'est
        </Turn>
        <Turn speaker="spk1 spk2" startTime="15.195" endTime="15.623" id="9">
          <Sync time="15.195" />
          <Who nb="1" />
          oui
          <Who nb="2" />
          ça
        </Turn>
        <Turn speaker="spk1" startTime="15.623" endTime="21.149" id="10">
          <Sync time="15.623" />
          il y a
          <anchor id="jmuzerelle_1370874715849" num="21">tout</anchor>
          à
          <anchor id="sduchon_1342951597443" num="22">l'intérieur</anchor>
          c'est classé par
          <anchor id="sduchon_1342951606650" num="23">massif</anchor>
          # [i] donc c'est sur
          <anchor id="sduchon_1342951617569" num="24">
            les hauteurs de
            <anchor id="sduchon_1342951633928" num="25">Grenoble</anchor>
          </anchor>
          en général hein dans
          <anchor id="sduchon_1342951649071" num="26">les stations</anchor>
          aussi
        </Turn>
        <Turn speaker="spk2" startTime="21.149" endTime="22.166" id="11">
          <Sync time="21.149" />
          d'accord merci bien
        </Turn>
        <Turn speaker="spk1" startTime="22.166" endTime="22.69" id="12">
          <Sync time="22.166" />
          <anchor id="sduchon_1342951797487" num="27">je</anchor>
          <anchor id="sduchon_1342951802959" num="28">vous</anchor>
          en prie
        </Turn>
        <Turn speaker="spk2" startTime="22.69" endTime="22.966" id="13">
          <Sync time="22.69" />
          au revoir
        </Turn>
        <Turn speaker="spk1" startTime="22.966" endTime="23.364" id="14">
          <Sync time="22.966" />
          au revoir
        </Turn>
