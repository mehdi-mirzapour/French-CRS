    <Episode>
      <Section type="report" startTime="0" endTime="18.075" topic="to1" id="1">
        <Turn startTime="0" endTime="1.251" speaker="spk1" id="1">
          <Sync time="0" />
          bonjour
        </Turn>
        <Turn speaker="spk2" startTime="1.251" endTime="2.957" id="2">
          <Sync time="1.251" />
          bonjour est ce que
          <anchor id="sduchon_1329831884885" num="1">vous</anchor>
          avez "
          <anchor id="sduchon_1329831664486" num="2">le 38</anchor>
          "
        </Turn>
        <Turn speaker="spk1" startTime="2.957" endTime="3.811" id="3">
          <Sync time="2.957" />
          non plus depuis
        </Turn>
        <Turn speaker="spk2 spk1" startTime="3.811" endTime="4.928" id="4">
          <Sync time="3.811" />
          <Who nb="1" />
          depuis plus d'accord
          <Who nb="2" />
          <anchor id="sduchon_1329831678796" num="3">ce matin</anchor>
          <anchor id="sduchon_1329831901444" num="4">il</anchor>
          faut venir
          <anchor id="sduchon_1329831691283" num="5">le lundi</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="4.928" endTime="6.058" id="5">
          <Sync time="4.928" />
          <anchor id="jmuzerelle_1371115476368" num="6">matin</anchor>
          et très tôt
        </Turn>
        <Turn speaker="spk2" startTime="6.058" endTime="8.138" id="6">
          <Sync time="6.058" />
          <anchor id="sduchon_1329831919355" num="7">la dernière fois</anchor>
          <anchor id="sduchon_1329832220043" num="8">on</anchor>
          <anchor id="sduchon_1329832236041" num="9">m'</anchor>
          a dit de venir
          <anchor id="sduchon_1329831932946" num="10">l'après-midi</anchor>
        </Turn>
        <Turn speaker="spk1" startTime="8.138" endTime="14.88" id="7">
          <Sync time="8.138" />
          ah ben non
          <anchor id="sduchon_1329832246840" num="11">on</anchor>
          <anchor id="sduchon_1329832259576" num="12">le</anchor>
          reçoit
          <anchor id="sduchon_1329831743744" num="13">lundi matin</anchor>
          donc e
          <anchor id="jmuzerelle_1368783305683" num="14">c'</anchor>
          est plus sûr de venir
          <anchor id="sduchon_1329831752488" num="15">le lundi matin</anchor>
          parce qu'en
          <anchor id="sduchon_1329831759847" num="16">ce moment</anchor>
          comme les
          <anchor id="sduchon_1329831772111" num="17">tous les étudiants</anchor>
          cherchent
          <anchor id="sduchon_1329831821625" num="18">des apparts</anchor>
          à louer c'est
        </Turn>
        <Turn speaker="spk2 spk1" startTime="14.88" endTime="16.012" id="8">
          <Sync time="14.88" />
          <Who nb="1" />
          c'est sûr
          <Who nb="2" />
          <anchor id="sduchon_1329831833728" num="19">l'euphorie générale</anchor>
        </Turn>
        <Turn speaker="spk2" startTime="16.012" endTime="17.415" id="9">
          <Sync time="16.012" />
          ok d'accord ben merci
        </Turn>
        <Turn speaker="spk1" startTime="17.415" endTime="18.075" id="10">
          <Sync time="17.415" />
          <anchor id="sduchon_1329832597783" num="20">je</anchor>
          <anchor id="sduchon_1329832604223" num="21">vous</anchor>
          en prie au revoir
        </Turn>
      </Section>
    </Episode>
